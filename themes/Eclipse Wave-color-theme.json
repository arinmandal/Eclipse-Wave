		"activityBarBadge.background": "#E94560", // Supernova red-pink
		"activityBarBadge.foreground": "#FFFFFF",

		// === Side Bar ===
		"sideBar.background": "#101325",
		"sideBar.foreground": "#B0B6C2",
		"sideBarTitle.foreground": "#FF79C6", // Nebula pink
		"sideBarSectionHeader.background": "#1A1F3B",

		// === Status Bar ===
		"statusBar.background": "#14182E",
		"statusBar.foreground": "#B0B6C2",
		"statusBar.border": "#202533",
		"statusBar.debuggingBackground": "#9D7CFF", // Cosmic violet
		"statusBar.noFolderBackground": "#0B0F1A",
		"statusBarItem.hoverBackground": "#1C1F3A",

		// === Panel ===
		"panel.background": "#101325",
		"panel.border": "#2C325A",
