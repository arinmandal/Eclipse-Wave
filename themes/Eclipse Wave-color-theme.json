		"terminalCursor.foreground": "#00E5FF",
		"terminal.ansiBlack": "#1E1E2E",
		"terminal.ansiRed": "#E94560",
		"terminal.ansiGreen": "#8AFF80",
		"terminal.ansiYellow": "#FFD166",
		"terminal.ansiBlue": "#61AFEF",
		"terminal.ansiMagenta": "#FF4DFF",
		"terminal.ansiCyan": "#00E5FF",
		"terminal.ansiWhite": "#E6E6F0",
		"terminal.ansiBrightBlack": "#6C7086",
		"terminal.ansiBrightWhite": "#FFFFFF",

		// === Lists, Trees & Breadcrumbs ===
		"list.hoverBackground": "#1C1F3A",
		"list.activeSelectionBackground": "#2A2F50",
		"list.activeSelectionForeground": "#FFFFFF",
		"list.focusBackground": "#202533",
		"list.inactiveSelectionBackground": "#14182E",
		"breadcrumb.foreground": "#7A8490",
		"breadcrumb.focusForeground": "#8AB4F8",
		"breadcrumb.activeSelectionForeground": "#9D7CFF",

		// === Buttons & Dropdowns ===
		"button.background": "#1C1F3A",
		"button.hoverBackground": "#2A2F50",
		"dropdown.background": "#14182E",
		"dropdown.foreground": "#B0B6C2",
		"dropdown.border": "#262C44",

		// === Peek View (Go to Definition popups) ===
		"peekView.border": "#2C325A",
		"peekViewEditor.background": "#101325",
		"peekViewEditor.matchHighlightBackground": "#4361EE55",
		"peekViewResult.background": "#14182E",
		"peekViewResult.matchHighlightBackground": "#4361EE55",
		"peekViewTitle.background": "#1A1F3B",

		// === Git Decorations ===
		"editorGutter.addedBackground": "#50FA7B55",
		"editorGutter.modifiedBackground": "#61AFEFAA",
		"editorGutter.deletedBackground": "#E9456055",

		// === Notifications ===
		"notificationCenterHeader.background": "#1A1F3B",
