		"inputValidation.warningBackground": "#FFCB6B33",
		"inputValidation.infoBackground": "#8BE9FD33",

		// === Debug / Testing ===
		"debugToolBar.background": "#101325",
		"debugIcon.breakpointForeground": "#E94560",
		"testing.iconPassed": "#50FA7B",
		"testing.iconFailed": "#FF5555",
		"testing.iconQueued": "#FFD166",

		// === Menu Bar ===
		"menubar.selectionBackground": "#1C1F3A",
		"menubar.selectionForeground": "#B0B6C2",
		"menubar.selectionBorder": "#262C44",

		// === Title Bar ===
		"titleBar.activeBackground": "#14182E",
		"titleBar.activeForeground": "#B0B6C2",
		"titleBar.inactiveBackground": "#0B0F1A",
		"titleBar.inactiveForeground": "#7A8490",

		// === Quick Input / Command Palette ===
		"quickInput.background": "#101325",
		"quickInput.foreground": "#B0B6C2",
		"quickInputList.focusBackground": "#1C1F3A",
		"pickerGroup.foreground": "#9D7CFF",
		"pickerGroup.border": "#262C44",

		// === Progress Bar ===
		"progressBar.background": "#9D7CFF",

		// === Editor Find Widget ===
		"editor.findMatchBackground": "#4361EE55",
		"editor.findMatchHighlightBackground": "#9D7CFF33",
		"editor.findRangeHighlightBackground": "#1C1F3A",

		// === Editor Squiggles ===
		"editorError.foreground": "#FF5555",
		"editorWarning.foreground": "#FFCB6B",
		"editorInfo.foreground": "#8BE9FD",

		// === Merge Conflicts ===
		"merge.currentHeaderBackground": "#61AFEFAA",
		"merge.incomingHeaderBackground": "#9D7CFFAA",
		"merge.currentContentBackground": "#61AFEF33",
		"merge.incomingContentBackground": "#9D7CFF33",

		// === Status Bar Items (Prominent) ===
		"statusBarItem.prominentBackground": "#9D7CFF",
		"statusBarItem.prominentHoverBackground": "#BD93F9",

		// === Peek View Titles ===
		"peekViewTitleLabel.foreground": "#B0B6C2",
		"peekViewTitleDescription.foreground": "#7A8490"
	},

	// === Token Colors (Syntax) ===
	"tokenColors": [
		{
			"name": "Comment",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": { "fontStyle": "italic", "foreground": "#6C7086" }
		},
		{
			"name": "Keyword",
			"scope": ["keyword", "storage.type", "storage.modifier"],
			"settings": { "foreground": "#9D7CFF" } // Galactic purple
		},
		{
			"name": "String",
			"scope": ["string", "constant.other.symbol"],
			"settings": { "foreground": "#A3BE8C" } // Aurora green
		},
		{
			"name": "Function",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"support.function"
			],
			"settings": { "foreground": "#82AAFF" } // Star blue
		},
		{
			"name": "Variable",
			"scope": ["variable", "string constant.other.placeholder"],
			"settings": { "foreground": "#E5C07B" } // Cosmic gold
		},
		{
			"name": "Number, Constant, Attribute",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"variable.parameter",
				"entity.other.attribute-name"
			],
			"settings": { "foreground": "#F78C6C" } // Solar orange
		},
		{
			"name": "Class, Type",
			"scope": [
				"entity.name.class",
				"support.class",
				"support.type",
				"storage.type"
			],
			"settings": { "foreground": "#FFD166" } // Golden star
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": { "foreground": "#E94560" } // Supernova red
		},
		{
			"name": "Operator",
			"scope": ["keyword.operator", "punctuation"],
			"settings": { "foreground": "#56B6C2" } // Plasma cyan
		},
		{
			"name": "Constant (Other)",
			"scope": ["constant", "constant.other"],
			"settings": { "foreground": "#BD93F9" } // Nebula violet
		},
		{
			"name": "Error",
			"scope": ["invalid", "invalid.illegal"],
			"settings": { "foreground": "#FF5370" } // Warning red
		},
		{
			"name": "Warning",
			"scope": ["markup.warning", "markup.changed"],
			"settings": { "foreground": "#FFCB6B" } // Warm yellow
		},
		{
			"name": "Selection/Highlight",
			"scope": ["text.selection", "highlight"],
			"settings": { "background": "#4361EE55" }
		},
		{
			"name": "Builtin/Attribute",
			"scope": [
				"support.type",
				"entity.other.attribute-name.class",
				"support.function.builtin"
			],
			"settings": { "foreground": "#FF79C6" } // Pink starlight
		}
	]
}
