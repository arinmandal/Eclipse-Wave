		"notificationsWarningIcon.foreground": "#FFCB6B",
		"notificationsInfoIcon.foreground": "#8BE9FD",

		// === Editor Group / Tabs enhancements ===
		"editorGroupHeader.tabsBackground": "#101325",
		"editorGroup.border": "#262C44",
		"tab.hoverBackground": "#1C1F3A",

		// === Minimap & Scrollbar ===
		"minimap.selectionHighlight": "#4361EE55",
		"scrollbarSlider.background": "#2A2F5033",
		"scrollbarSlider.hoverBackground": "#2A2F5066",
		"scrollbarSlider.activeBackground": "#2A2F5088",

		// === Input Controls ===
		"input.background": "#14182E",
		"input.foreground": "#B0B6C2",
