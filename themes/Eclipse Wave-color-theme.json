	// === Token Colors (Syntax) ===
	"tokenColors": [
		// ──────────────────────────────────────────────────────────
		//  SECTION 1: CORE ELEMENTS
		//  Basic text, comments, strings, and numbers.
		// ──────────────────────────────────────────────────────────
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#7A7F95",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#A1C682" // Bright Aurora Green
			}
		},
		{
			"name": "Numbers and Booleans",
			"scope": [
				"constant.numeric",
				"constant.language"
			],
			"settings": {
				"foreground": "#E5A574" // Warm Amber (softer)
			}
		},
		// ──────────────────────────────────────────────────────────
		//  SECTION 2: KEYWORDS, STORAGE & OPERATORS
		//  Language keywords, storage types, modifiers, and operators.
		// ──────────────────────────────────────────────────────────
		{
			"name": "Keywords",
			"scope": [
				"keyword.control",
				"keyword.other"
			],
			"settings": {
				"foreground": "#9D7CFF", // Galactic Purple
				"fontStyle": "italic"
			}
		},
		{
			"name": "Storage Types & Modifiers (e.g., const, let, static, async)",
			"scope": [
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#9D7CFF", // Galactic Purple
				"fontStyle": "italic"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#C792EA" // lavender-purple
			}
		},
		{
			"name": "Punctuation (Brackets, Commas, Semicolons)",
			"scope": [
				"punctuation.separator",
				"punctuation.terminator",
				"punctuation.section",
				"punctuation.accessor"
			],
			"settings": {
				"foreground": "#A1ADC8" // Comment Gray
			}
		},
		// ──────────────────────────────────────────────────────────
		//  SECTION 3: FUNCTIONS & CLASSES
		//  Declarations, calls, types, and inheritance.
		// ──────────────────────────────────────────────────────────
		{
			"name": "Function Declarations & Calls",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"support.function"
			],
			"settings": {
				"foreground": "#82AAFF" // Star Blue
			}
		},
		{
			"name": "Class & Type Declarations",
			"scope": [
				"entity.name.class",
				"entity.name.type",
				"support.class",
				"support.type"
			],
			"settings": {
				"foreground": "#D7BA7D" // Cosmic Gold (balanced)
			}
		},
		{
			"name": "Inherited Class",
			"scope": "entity.other.inherited-class",
			"settings": {
				"foreground": "#D7BA7D", // Cosmic Gold (balanced)
				"fontStyle": "italic"
			}
		},
		// ──────────────────────────────────────────────────────────
		//  SECTION 4: VARIABLES, CONSTANTS & PARAMETERS
		//  Variables, properties, constants, and function arguments.
		// ──────────────────────────────────────────────────────────
		{
			"name": "Variables",
			"scope": [
				"variable.other.object",
				"variable.other.readwrite"
			],
			"settings": {
				"foreground": "#E5A574"
			}
		},
		{
			"name": "Object Properties & Keys",
			"scope": [
				"variable.other.property",
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#7ED6D4" // Bracket Cyan
			}
		},
		{
			"name": "Language Constants & Built-in Variables (e.g., 'this', 'null')",
			"scope": [
				"variable.language",
				"support.constant"
			],
			"settings": {
				"foreground": "#F78C6C" // Solar Orange
			}
		},
		{
			"name": "Function Parameters",
			"scope": "variable.parameter",
			"settings": {
				"foreground": "#D7BA7D", // Cosmic Gold
				"fontStyle": "italic"
			}
		},
		// ──────────────────────────────────────────────────────────
		//  SECTION 5: HTML & CSS
		//  Tags, attributes, IDs, and class names.
		// ──────────────────────────────────────────────────────────
		{
			"name": "HTML Tags",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#E17888" // Aurora Rose
			}
		},
		{
			"name": "HTML/CSS Attributes",
			"scope": [
				"entity.other.attribute-name",
				"support.type.vendored.property-name"
			],
			"settings": {
				"foreground": "#FF79C6" // Pink Starlight
			}
		},
		{
			"name": "HTML/CSS Attribute Values (e.g. class names)",
			"scope": "string.quoted",
			"settings": {
				"foreground": "#B4D695" // Bright Aurora Green
			}
		},
		{
			"name": "CSS Selectors (ID, Class)",
			"scope": [
				"entity.other.attribute-name.id",
				"entity.other.attribute-name.class"
			],
			"settings": {
				"foreground": "#D7BA7D" // Cosmic Gold (balanced)
			}
		},
		// ──────────────────────────────────────────────────────────
		//  SECTION 6: MARKDOWN
		//  Specific styles for Markdown files.
		// ──────────────────────────────────────────────────────────
		{
			"name": "Markdown - Headings",
			"scope": "entity.name.section.markdown",
			"settings": {
				"foreground": "#9D7CFF", // Galactic Purple
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown - Bold",
			"scope": "markup.bold.markdown",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown - Italic",
			"scope": "markup.italic.markdown",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markdown - Link URL",
			"scope": "markup.underline.link.markdown",
			"settings": {
				"foreground": "#82AAFF", // Star Blue
				"fontStyle": "underline"
			}
		},
		{
			"name": "Markdown - Inline Code",
			"scope": "markup.inline.raw.string.markdown",
			"settings": {
				"foreground": "#B4D695" // Aurora Green
			}
		},
		// ──────────────────────────────────────────────────────────
		//  SECTION 7: LANGUAGE SPECIFICS
		//  Rules for specific languages like React (JSX) and Python.
		// ──────────────────────────────────────────────────────────
		{
			"name": "React/JSX - Component Tags",
			"scope": "support.class.component",
			"settings": {
				"foreground": "#FFD166" // Golden Star
			}
		},
		{
			"name": "Python - Decorators",
			"scope": "entity.name.function.decorator",
			"settings": {
				"foreground": "#C792EA" // lavender-purple
			}
		},
		{
			"name": "Python - f-string interpolation",
			"scope": [
				"constant.character.format.placeholder.begin.python",
				"constant.character.format.placeholder.end.python"
			],
			"settings": {
				"foreground": "#F78C6C" // Solar Orange
			}
		},
		// ──────────────────────────────────────────────────────────
		//  SECTION 8: INVALID & DEPRECATED
		//  Highlighting for errors and outdated code.
		// ──────────────────────────────────────────────────────────
		{
			"name": "Invalid or Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"foreground": "#FF5370",
				"fontStyle": "italic bold"
			}
		},
		{
			"name": "Deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"foreground": "#E5C07B", // Cosmic Gold
				"fontStyle": "italic strikethrough"
			}
		},
		// ──────────────────────────────────────────────────────────
		//  SECTION 9: ADDITIONAL LANGUAGE SUPPORT
		//  Regex, TypeScript types, JSON/YAML, Template strings
		// ──────────────────────────────────────────────────────────
		{
			"name": "Regular Expressions",
			"scope": [
				"string.regexp",
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#FF79C6"
			}
		},
		{
			"name": "Type Annotations",
			"scope": [
				"entity.name.type.module",
				"support.type.primitive"
			],
			"settings": {
				"foreground": "#7CC8DE"
			}
		},
		{
			"name": "JSON/YAML Keys",
			"scope": [
				"support.type.property-name.json",
				"entity.name.tag.yaml"
			],
			"settings": {
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "Template String Interpolation",
			"scope": [
				"punctuation.definition.template-expression"
			],
			"settings": {
				"foreground": "#F78C6C"
			}
		},
		{
			"name": "Macro/Attribute (Rust, C++)",
			"scope": [
				"entity.name.function.macro",
				"meta.attribute"
			],
			"settings": {
				"foreground": "#C792EA"
			}
		}
	],
	// === Semantic Highlighting (TypeScript, Rust, etc.) ===
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"variable.readonly": {
			"foreground": "#E5A574",
			"fontStyle": "bold"
		},
		"parameter": {
			"foreground": "#D7BA7D",
			"fontStyle": "italic"
		},
		"function.declaration": {
			"foreground": "#82AAFF"
		},
		"class": {
			"foreground": "#FFD166"
		},
		"interface": {
			"foreground": "#7ED6D4"
		},
		"enum": {
			"foreground": "#C792EA"
		},
		"enumMember": {
			"foreground": "#FFAC40"
		},
		"namespace": {
			"foreground": "#FF79C6"
		},
		"type": {
			"foreground": "#FFD166"
		},
		"property.readonly": {
			"fontStyle": "bold"
		}
	}
}